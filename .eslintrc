{
  "extends": [
    "standard",
    "standard-react",
    "plugin:unicorn/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react",
    "prettier/unicorn"
  ],
  "plugins": ["jsx-a11y", "prettier", "unicorn"],
  "env": {
    "es6": true
  },
  "rules": {
    "no-console": ["error", { "allow": ["info", "warn", "error"] }],
    "prettier/prettier": "error"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "src"]
      }
    }
  }
}
